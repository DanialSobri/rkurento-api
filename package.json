{
  "name": "rkurento-api",
  "version": "1.0.0",
  "description": "RKurento API",
  "main": "lib/server.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "dev": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/server.js\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "testAll": "mocha -r ts-node/register ./lib/**/tests/*.test.ts",
    "test": "mocha -r ts-node/register ./lib/**/tests/rkurento-api.test.ts",
    "coverage": "nyc -r text -e .ts -x \"./lib/**/tests/*.test.ts\" npm run test"
  },
  "author": "Danial Sobri",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^5.1.0",
    "https": "^1.0.0",
    "ip": "^1.1.8",
    "kurento-client": "^6.16.0",
    "mongoose": "^6.4.0",
    "nanoid": "^3.3.4",
    "rimraf": "^3.0.2",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/helmet": "^4.0.0",
    "@types/ip": "^1.1.0",
    "@types/kurento-client": "^6.14.8",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.44",
    "@types/ws": "^8.5.3",
    "chai": "^4.3.6",
    "concurrently": "^7.2.2",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.4"
  }
}
